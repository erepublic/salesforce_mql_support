    <assignments>
        <name>Assign_Course_Enrollment_Name</name>
        <label>Assign Course Enrollment Name</label>
        <locationX>176</locationX>
        <locationY>395</locationY>
        <assignmentItems>
            <assignToReference>$Record.Name</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>f_Course_Enrollment_Name</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>var_Course_Id</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Course__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference
      >$Record.Standardized_Course_Name__c</assignToReference>
            <operator>Assign</operator>
