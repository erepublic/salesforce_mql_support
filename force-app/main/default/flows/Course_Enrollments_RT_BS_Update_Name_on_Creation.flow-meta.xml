                <elementReference
        >Get_Course.Standardized_Course_Name__c</elementReference>
            </value>
        </assignmentItems>
    </assignments>
    <environments>Default</environments>
    <formulas>
        <name>f_Course_Enrollment_Name</name>
        <dataType>String</dataType>
        <expression
    >LEFT({!$Record.Course_Account__r.Name},{!f_Course_Name_Length}) &amp; 
&quot; - &quot; &amp; 
TEXT(YEAR({!$Record.Course_Enrollment_Date__c})) &amp; 
&quot; - &quot; &amp; {!Get_Course.Course_Nickname__c} &amp; &quot; - &quot; &amp; 
IF(AND({!$Record.Delivery_Contract__r.ContractNumber}=&quot;&quot;,{!$Record.Course_Account__c}=&quot;0013000000LtZ3UAAV&quot;),
    &quot;Internal&quot;, 
    IF({!$Record.Delivery_Contract__r.ContractNumber}=&quot;&quot;, 
        &quot;No Contract&quot;, 
        {!$Record.Delivery_Contract__r.ContractNumber}))</expression>
    </formulas>
    <formulas>
        <name>f_Course_Name_Length</name>
        <dataType>Number</dataType>
        <expression>80-LEN(&quot; - &quot; &amp; 
TEXT(YEAR({!$Record.Course_Enrollment_Date__c})) &amp; 
&quot; - &quot; &amp; {!Get_Course.Course_Nickname__c} &amp; &quot; - &quot; &amp; 
IF(AND({!$Record.Delivery_Contract__r.ContractNumber}=&quot;&quot;,{!$Record.Course_Account__c}=&quot;0013000000LtZ3UAAV&quot;),
    &quot;Internal&quot;, 
    IF({!$Record.Delivery_Contract__r.ContractNumber}=&quot;&quot;, 
        &quot;No Contract&quot;, 
        {!$Record.Delivery_Contract__r.ContractNumber})))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Course_Nickname</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Course__c}, 
         &quot;aC45Y000000slmlSAA&quot;, &quot;HED Free Preview&quot;, 
         &quot;aC45Y000000slmwSAA&quot;, &quot;HED&quot;, 
         &quot;aC45Y000000sln0SAA&quot;, &quot;SLG Free Preview&quot;, 
         &quot;aC45Y000000slmvSAA&quot;, &quot;K12 &amp; HED&quot;, 
         &quot;aC45Y000000slmhSAA&quot;, &quot;K12 Free Preview&quot;, 
         &quot;aC45Y000000slmgSAA&quot;, &quot;K12&quot;, 
         &quot;aC45Y000000slmqSAA&quot;, &quot;K12 &amp; HED Free Preview&quot;, 
         &quot;aC45Y000000slmSSAQ&quot;, &quot;SLG&quot;,
         &quot;[Insert Course Nickname]&quot;)</expression>
    </formulas>
    <interviewLabel
