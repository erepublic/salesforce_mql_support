    <decisions>
        <name>Is_Created_Not_Active_Opp</name>
        <label>Is Created &amp; Not Active Opp?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Assign_MQL_Name_Variables</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Update_Contact</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.CreatedDate</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Contact__r.MQL_Lifecycle_Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Active Opportunity</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Contact__r.MQL_Lifecycle_Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Qualified Lead</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Contact</targetReference>
            </connector>
            <label>Yes, Update Contact</label>
        </rules>
    </decisions>
    <decisions>
        <name>Save_MQL_Name</name>
        <label>Save MQL Name?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Assign_Super_Trimmed_MQL_Name</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Super Trim Product</defaultConnectorLabel>
        <rules>
            <name>Yes_Save_MQL_Name</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>f_Check_Length</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>120.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_MQL</targetReference>
            </connector>
            <label>Yes, Save MQL Name</label>
        </rules>
        <rules>
            <name>No_Trim_Account</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Account_Trimmed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>f_Account_Name_Length</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>35.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Trim_Account_Name</targetReference>
            </connector>
            <label>No,  Trim Account</label>
        </rules>
        <rules>
            <name>No_Trim_Contact_Name</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference
        >var_Contact_Name_Trimmed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>f_Contact_Name_Length</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>31.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Trim_Contact_Name</targetReference>
            </connector>
            <label>No, Trim Contact Name</label>
        </rules>
        <rules>
            <name>No_Trim_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Date_Trimmed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Trim_Date</targetReference>
            </connector>
            <label>No, Trim Date</label>
        </rules>
        <rules>
            <name>No_Trim_Product</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Product_Trimmed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>f_Length_of_Product</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>28.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Trim_Product</targetReference>
            </connector>
            <label>No, Trim Product</label>
        </rules>
    </decisions>
    <decisions>
        <name>Update_MQL_Name</name>
        <label>Update MQL Name?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Update_MQL_Name</name>
            <conditionLogic>(1 AND 2) OR 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.MQL_Name__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.CreatedDate</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.CreatedDate</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_MQL_Name_If_needed</targetReference>
            </connector>
            <label>Yes, Update MQL Name</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>f_Account_Name_Length</name>
        <dataType>Number</dataType>
        <expression>LEN({!var_Account_Name})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Check_Length</name>
        <dataType>Number</dataType>
        <expression>LEN({!var_MQL_Name})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Contact_Name_Length</name>
        <dataType>Number</dataType>
        <expression
    >LEN({!var_Contact_First_Name})+1+LEN({!var_Contact_Last_Name})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Lead_Source</name>
        <dataType>String</dataType>
        <expression>CASE(TEXT({!$Record.Lead_Source__c}), 
&quot;Submitted a Form&quot;, &quot;Form Submit&quot;,
&quot;Events Portal&quot;, &quot;E. Portal&quot;,
&quot;Email&quot;, &quot;Email&quot;,
&quot;Registered for a Webinar&quot;, &quot;Registration&quot;,
&quot;Registered for an Event&quot;, &quot;Registration&quot;,
&quot;Manual Marketing Review&quot;, &quot;Mkting Review&quot;,
&quot;Fit and Behavior Threshold Reached&quot;, &quot;Threshold&quot;,
&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_Length_of_Product</name>
        <dataType>Number</dataType>
        <expression>LEN({!var_Product})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_MQL_Date</name>
        <dataType>String</dataType>
        <expression
    >TEXT(MONTH({!$Record.MQL_Date__c})) &amp; &quot;/&quot; &amp; 
TEXT(DAY({!$Record.MQL_Date__c})) &amp; &quot;/&quot; &amp; 
TEXT(YEAR({!$Record.MQL_Date__c}))  &amp; &quot; - &quot;</expression>
    </formulas>
    <formulas>
        <name>f_MQL_Name</name>
        <dataType>String</dataType>
        <expression>{!var_Account_Name} &amp; &quot; - &quot; &amp; 
{!var_Lead_Source} &amp; &quot; - &quot; &amp; 
{!var_Product} &amp; &quot; - &quot; &amp; 
{!var_MQL_Date} &amp; 
{!var_Contact_First_Name} &amp; &quot; &quot; &amp; {!var_Contact_Last_Name}</expression>
    </formulas>
    <formulas>
        <name>f_Product_Display</name>
        <dataType>String</dataType>
        <expression>CASE(TEXT({!$Record.Product__r.Product_Type__c}), 
&quot;Events&quot;, TEXT({!$Record.Product__r.Sponsorship__c}) &amp; &quot; - &quot; &amp; {!$Record.Product__r.EE_Event__r.Nickname__c}, 
&quot;Custom Events&quot;, TEXT({!$Record.Product__r.Sponsorship__c}) &amp; &quot; - &quot; &amp; {!$Record.Product__r.EE_Event__r.Nickname__c}, 
&quot;Content Studio&quot;, TEXT({!$Record.Product__r.Product_Type__c}) &amp; &quot; - &quot; &amp; TEXT({!$Record.Product__r.Product_Category__c}), 
&quot;Demand Gen&quot;, TEXT({!$Record.Product__r.Product_Type__c}) &amp; &quot; - &quot; &amp; TEXT({!$Record.Product__r.Product_Category__c}), 
&quot;Web Products&quot;, TEXT({!$Record.Product__r.Product_Type__c}) &amp; &quot; - &quot; &amp; TEXT({!$Record.Product__r.Product_Category__c}), 
&quot;Program&quot;, {!$Record.Product__r.Program_Name__c}, 
&quot;Research&quot;, &quot;Research&quot;, 
IF(LEN({!$Record.Product_Name__c})&gt;60, LEFT({!$Record.Product_Name__c}, 60), {!$Record.Product_Name__c}))</expression>
    </formulas>
    <formulas>
        <name>f_Super_Trimmed_Product</name>
        <dataType>String</dataType>
        <expression
    >LEFT({!var_Product}, LEN({!var_Product})-(LEN({!var_MQL_Name})-120))</expression>
    </formulas>
    <formulas>
        <name>f_Trimmed_Account_Name</name>
        <dataType>String</dataType>
        <expression>LEFT({!var_Account_Name}, 35)</expression>
    </formulas>
    <formulas>
        <name>f_Trimmed_Contact_First_Name</name>
        <dataType>String</dataType>
        <expression>LEFT({!var_Contact_First_Name}, 15)</expression>
    </formulas>
    <formulas>
        <name>f_Trimmed_First_Name</name>
        <dataType>String</dataType>
        <expression>LEFT({!var_Contact_First_Name}, 15)</expression>
    </formulas>
    <formulas>
        <name>f_Trimmed_Last_Name</name>
        <dataType>String</dataType>
        <expression>LEFT({!var_Contact_Last_Name}, 15)</expression>
    </formulas>
    <formulas>
        <name>f_Trimmed_Product</name>
        <dataType>String</dataType>
        <expression>LEFT({!var_Product}, 28)</expression>
    </formulas>
