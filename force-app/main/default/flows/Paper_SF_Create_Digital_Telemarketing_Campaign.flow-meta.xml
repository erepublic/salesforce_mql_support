    <formulas>
        <name>f_Formula_for_Brand</name>
        <dataType>String</dataType>
        <expression>IF(OR(
BEGINS({!var_Brand_from_Paper}, &quot;GT&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;EE&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;EM&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;ER&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;II&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;DC&quot;)
),
LEFT({!var_Brand_from_Paper}, 2),
IF(OR(
BEGINS({!var_Brand_from_Paper}, &quot;GOV&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;CDG&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;CDE&quot;),
BEGINS({!var_Brand_from_Paper}, &quot;NAV&quot;)
),
LEFT({!var_Brand_from_Paper}, 3),
IF(
BEGINS({!var_Brand_from_Paper}, &quot;PCIO&quot;)
,LEFT({!var_Brand_from_Paper}, 4),&quot;&quot;)))</expression>
    </formulas>
