    <decisions>
        <name>Contact_Fit_Behavior</name>
        <label>Contact Fit / Behavior</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Loop_through_contacts</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_High_Behavior_30</name>
            <conditionLogic
      >1 AND ((2 AND 3) OR (4 AND (5 OR 6))) AND NOT 7</conditionLogic>
            <conditions>
                <leftValueReference
        >Loop_through_contacts.HubSpot_Private_Sector_Behavior_Score__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>30.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >Loop_through_contacts.Contact_Fit_Threshold__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Medium</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Company_Fit_Threshold__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Medium</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >Loop_through_contacts.Contact_Fit_Threshold__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>High</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Company_Fit_Threshold__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Medium</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Company_Fit_Threshold__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Low</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Contacts_with_Open_Opps</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference
          >Loop_through_contacts.Id</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Contact_High_Behavior</targetReference>
            </connector>
            <label>Yes - High Behavior (30+)</label>
        </rules>
        <rules>
            <name>Yes_Medium_Behavior_20</name>
            <conditionLogic>1 AND 2 AND (3 OR 4) AND NOT 5</conditionLogic>
            <conditions>
                <leftValueReference
        >Loop_through_contacts.HubSpot_Private_Sector_Behavior_Score__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Company_Fit_Threshold__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>High</stringValue>
                </rightValue>
            </conditions>
            <conditions>
