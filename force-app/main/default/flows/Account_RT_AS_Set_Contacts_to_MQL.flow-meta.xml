    <recordLookups>
        <name>Get_Potential_MQL_Contacts</name>
        <label>Get Potential MQL Contacts</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound
    >false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Contacts_Found</targetReference>
        </connector>
        <filterLogic
    >1 AND 2 AND 3 AND (4 OR 5) AND (6 OR 7) AND 8 AND (9 OR 10 OR 11) AND 12 AND 13</filterLogic>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>012300000008caRAAQ</stringValue>
            </value>
        </filters>
        <filters>
            <field>AccountId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>HubSpot_Private_Sector_Behavior_Date__c</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <elementReference>f_Today_Minus_7_Days</elementReference>
            </value>
        </filters>
        <filters>
            <field>Last_Date_MQL__c</field>
            <operator>LessThanOrEqualTo</operator>
            <value>
                <elementReference>f_Today_Minus_30_Days</elementReference>
            </value>
        </filters>
        <filters>
            <field>Last_Date_MQL__c</field>
            <operator>EqualTo</operator>
        </filters>
        <filters>
            <field>Last_Date_Recycled__c</field>
            <operator>LessThanOrEqualTo</operator>
            <value>
                <elementReference>f_Today_Minus_30_Days</elementReference>
            </value>
        </filters>
        <filters>
            <field>Last_Date_Recycled__c</field>
            <operator>EqualTo</operator>
        </filters>
        <filters>
            <field>Set_to_MQL__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>MQL_Lifecycle_Stage__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Unengaged</stringValue>
            </value>
        </filters>
        <filters>
            <field>MQL_Lifecycle_Stage__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Engaged</stringValue>
            </value>
        </filters>
        <filters>
            <field>MQL_Lifecycle_Stage__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Recycled</stringValue>
            </value>
        </filters>
        <filters>
            <field>Private_Sector_Non_Qual__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>HubSpot_Private_Sector_Behavior_Score__c</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <numberValue>20.0</numberValue>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>Contact</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
