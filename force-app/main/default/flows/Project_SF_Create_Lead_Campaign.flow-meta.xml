    <formulas>
        <name>f_Brand_from_Project</name>
        <dataType>String</dataType>
        <expression>IF(LEFT(LOWER({!recordId.Name}),8)=&quot;t-mobile&quot;, 
TRIM(MID(
    RIGHT({!recordId.Name}, LEN({!recordId.Name})-8), 
    FIND(&quot;-&quot;, RIGHT({!recordId.Name}, LEN({!recordId.Name})-8)) + 2, 
    FIND(&quot;-&quot;, RIGHT({!recordId.Name}, LEN({!recordId.Name})-8), FIND(&quot;-&quot;, RIGHT({!recordId.Name}, LEN({!recordId.Name})-8)) + 1) - FIND(&quot;-&quot;, RIGHT({!recordId.Name}, LEN({!recordId.Name})-8)) - 2
))
, 
TRIM(MID(
    {!recordId.Name}, 
    FIND(&quot;-&quot;, {!recordId.Name}) + 2, 
    FIND(&quot;-&quot;, {!recordId.Name}, FIND(&quot;-&quot;, {!recordId.Name}) + 1) - FIND(&quot;-&quot;, {!recordId.Name}) - 2
)))</expression>
    </formulas>
    <formulas>
        <name>f_Campaign_Name</name>
        <dataType>String</dataType>
        <expression
    >{!Screen_Brand} &amp; &quot; - &quot; &amp; {!recordId.Client__r.Name} &amp; &quot; - &quot; &amp; {!recordId.Contract__r.ContractNumber} &amp; &quot; - &quot; &amp; {!Screen_Campaign_Type} &amp; &quot; - Demand Gen&quot;</expression>
    </formulas>
    <formulas>
        <name>f_End_Date_by_Type</name>
        <dataType>Date</dataType>
        <expression
    >IF({!Screen_Campaign_Type}={!Choice_Email}, {!Screen_End_Date_Email_Type}, {!Screen_End_Date})</expression>
    </formulas>
    <formulas>
        <name>f_Goal</name>
        <dataType>Number</dataType>
        <expression
    >IF(AND({!recordId.Paper__c}&lt;&gt;null,{!recordId.Event__c}&lt;&gt;null), null, 
IF({!recordId.Paper__c}&lt;&gt;null, {!recordId.Paper__r.Lead_Guarantee__c}, 
IF({!recordId.Event__c}&lt;&gt;null, {!recordId.Event__r.Targeted_Registration__c}, 
null)))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_Link_To_Campaign</name>
        <dataType>String</dataType>
        <expression
    >&quot;/lightning/r/Campaign/&quot; &amp; {!var_Campaign_to_Create.Id} &amp; &quot;/view&quot;</expression>
    </formulas>
    <formulas>
        <name>f_Telemarketing_Contract</name>
        <dataType>String</dataType>
        <expression
    >IF({!Screen_Campaign_Type}={!Choice_Telemarketing}, {!recordId.Contract__c}, null)</expression>
    </formulas>
    <formulas>
        <name>f_Web_Vendor_Lead_Contract</name>
        <dataType>String</dataType>
        <expression
    >IF({!Screen_Campaign_Type}={!Choice_Web_Vendor_Lead}, {!recordId.Contract__c}, null)</expression>
    </formulas>
