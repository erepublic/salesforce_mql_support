    <formulas>
        <name>f_Adbd_Campaign_Date_Greater_than_56</name>
        <dataType>Date</dataType>
        <expression>CASE( 
MOD({!$Record.Advisory_Board_1__c} - DATE( 1900, 1, 7 ), 7 ), 
0, {!$Record.Advisory_Board_1__c} - 55,
1, {!$Record.Advisory_Board_1__c} - 56, 
2, {!$Record.Advisory_Board_1__c} - 56, 
3, {!$Record.Advisory_Board_1__c} - 56, 
4, {!$Record.Advisory_Board_1__c} - 56, 
5, {!$Record.Advisory_Board_1__c} - 56, 
6, {!$Record.Advisory_Board_1__c} - 54,
null 
)</expression>
    </formulas>
    <formulas>
        <name>f_Adbd_Campaign_Date_Less_than_56</name>
        <dataType>Date</dataType>
        <expression>CASE( 
MOD(TODAY()- DATE( 1900, 1, 7 ), 7 ), 
0, TODAY() + 5,
1, TODAY() + 7, 
2, TODAY() + 7, 
3, TODAY() + 7, 
4, TODAY() + 7, 
5, TODAY() + 7, 
6, TODAY() + 6,
null 
)</expression>
    </formulas>
