        </rules>
    </decisions>
    <decisions>
        <name>Is_Advisory_Board_1_Less_than_56_Days_from_now</name>
        <label>Is Advisory Board #1 Less than 56 Days from now?</label>
        <locationX>182</locationX>
        <locationY>431</locationY>
        <defaultConnector>
            <targetReference
      >Assign_AdBd_Email_Campaign_Greater_or_Equal</targetReference>
        </defaultConnector>
        <defaultConnectorLabel
    >Greater or Equal to 56 Days from Now</defaultConnectorLabel>
        <rules>
            <name>Less_than_56_Days_from_Now</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference
        >$Record.Advisory_Board_1__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>f_Today_Plus_56</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference
        >Assign_AdBd_Email_Campaign_Less_Than</targetReference>
            </connector>
            <label>Less than 56 Days from Now</label>
        </rules>
    </decisions>
