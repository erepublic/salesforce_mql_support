    <decisions>
        <name>Event_Start_Date_Greater_than_Today</name>
        <label>Event Start Date Greater than Today?</label>
        <locationX>380</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>No, Event has Passed</defaultConnectorLabel>
        <rules>
            <name>Yes_Event_Start_Date_is_Greater_than_Today</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference
        >$Record.Event_Start_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Flow.CurrentDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference
        >Is_Advisory_Board_1_Less_than_56_Days_from_now</targetReference>
