&amp;TEXT(MONTH({!$Flow.CurrentDate})) &amp; &quot;/&quot; &amp; TEXT(DAY({!$Flow.CurrentDate})) &amp; &quot;/&quot; &amp; TEXT(YEAR({!$Flow.CurrentDate}))
