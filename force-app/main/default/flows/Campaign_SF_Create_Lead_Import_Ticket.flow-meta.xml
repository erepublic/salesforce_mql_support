        <expression>{!$Flow.CurrentDate}+2</expression>
    </formulas>
    <formulas>
        <name>f_Lead_Type_Magazine</name>
        <dataType>String</dataType>
        <expression
    >IF(TEXT({!Get_Triggering_Campaign.Type})=&quot;Web Vendor Lead&quot;, {!Choice_Web_Vendor_Magazine_Subscribers}, IF(TEXT({!Get_Triggering_Campaign.Type})=&quot;Telemarketing&quot;, {!Choice_Telemarketing_Magazine_Subscribers}, &quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>f_Lead_Type_Webinar_Paper</name>
        <dataType>String</dataType>
        <expression
    >IF(TEXT({!Get_Triggering_Campaign.Type})=&quot;Web Vendor Lead&quot;, {!Choice_Web_Vendor_Leads}, 
IF(TEXT({!Get_Triggering_Campaign.Type})=&quot;Telemarketing&quot;, {!Choice_Telemarketing_Leads}, 
IF(CONTAINS({!Get_Triggering_Campaign.Name},&quot;HubSpot&quot;), {!Choice_Hubspot_Leads}, &quot;&quot;
)
)
)</expression>
    </formulas>
    <formulas>
        <name>f_Link_to_Created_Ticket</name>
        <dataType>String</dataType>
        <expression
    >&quot;/lightning/r/Ticket__c/&quot; &amp; {!var_Ticket_to_Create.Id} &amp; &quot;/view&quot;</expression>
    </formulas>
    <formulas>
        <name>f_Related_Attached_Documents_Link</name>
        <dataType>String</dataType>
        <expression
    >&quot;/lightning/r/Ticket__c/&quot; &amp; {!var_Ticket_to_Create.Id} &amp; &quot;/related/AttachedContentDocuments/view&quot;</expression>
    </formulas>
    <formulas>
        <description
    >Build the Ticket Title in the format of &quot;Update - {Object} - {Campaign Name} - Lead Import&quot;
Add Today&apos;s Date for Publications &amp; Newsletters.</description>
        <name>f_Ticket_Title</name>
        <dataType>String</dataType>
        <expression
    >&quot;Update - &quot; &amp; {!Screen_Select_Lead_Object} &amp; &quot; - &quot; &amp; {!Get_Triggering_Campaign.Name} &amp; &quot; - Lead Import&quot; &amp;
IF(OR({!Screen_Select_Lead_Object}={!Object_Choice_Magazine}, 
          {!Screen_Select_Lead_Object}={!Object_Choice_Newsletter}), 
           &quot; - &quot; &amp; TEXT(Month({!$Flow.CurrentDate})) &amp; &quot;/&quot; &amp; TEXT(DAY({!$Flow.CurrentDate})) &amp; &quot;/&quot; &amp; TEXT(YEAR({!$Flow.CurrentDate})) 
           ,  &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_Which_Description_Text_Template</name>
        <dataType>String</dataType>
        <expression>IF(AND(
            {!Screen_Select_Lead_Object}={!Object_Choice_Webinar},
            {!Screen_Lead_Import_Type_Paper_Webinar}={!Choice_Web_Vendor_Leads}), 
            {!Text_Template_Web_Vendor_Leads_Webinar}, 
IF(AND(
            {!Screen_Select_Lead_Object}={!Object_Choice_Paper},
            {!Screen_Lead_Import_Type_Paper_Webinar}={!Choice_Web_Vendor_Leads}), 
            {!Text_Template_Web_Vendor_Leads_Paper}, 
 IF(AND(
            OR({!Screen_Select_Lead_Object}={!Object_Choice_Paper},
                  {!Screen_Select_Lead_Object}={!Object_Choice_Webinar}),
            {!Screen_Lead_Import_Type_Paper_Webinar}={!Choice_Telemarketing_Leads}), 
            {!Text_Template_Telemarketing_Leads_Paper_Webinar}, 
IF(AND(
            OR({!Screen_Select_Lead_Object}={!Object_Choice_Paper},
                  {!Screen_Select_Lead_Object}={!Object_Choice_Webinar}),
            {!Screen_Lead_Import_Type_Paper_Webinar}={!Choice_Hubspot_Leads}), 
            {!Text_Template_HubSpot_Leads_Paper_Webinar}, 
 IF(
     {!Screen_Select_Lead_Object}={!Object_Choice_Newsletter},
     {!Text_Template_Web_Vendor_Lead_Newsletter_Opt_Ins}, 
IF(
    AND({!Screen_Select_Lead_Object}={!Object_Choice_Magazine},
            {!Screen_Lead_Import_Type_Publication}={!Choice_Web_Vendor_Magazine_Subscribers}), 
            {!Text_Template_Web_Vendor_Magazine_Subscribers}, 
IF(
    AND({!Screen_Select_Lead_Object}={!Object_Choice_Magazine},
            {!Screen_Lead_Import_Type_Publication}={!Choice_Telemarketing_Magazine_Subscribers}), 
            {!Text_Template_Telemarketing_Magazine_Subscribers}, 
&quot;&quot;)))))))</expression>
    </formulas>
    <formulas>
        <name>f_Which_Object_Choice</name>
        <dataType>String</dataType>
        <expression
    >IF(AND({!Get_Triggering_Campaign.Paper__c}&lt;&gt;null,{!Get_Triggering_Campaign.Event__c}&lt;&gt;null), &quot;&quot;, 
IF(OR(CONTAINS({!Get_Triggering_Campaign.Name},&quot;Paper&quot;),
          TEXT({!Get_Triggering_Campaign.Promotion_Type__c})=&quot;Paper&quot;), {!Object_Choice_Paper}, 
IF(OR(CONTAINS({!Get_Triggering_Campaign.Name},&quot;Webinar&quot;),
          TEXT({!Get_Triggering_Campaign.Promotion_Type__c})=&quot;Webinar&quot;), {!Object_Choice_Webinar}, 
IF(OR(CONTAINS({!Get_Triggering_Campaign.Name},&quot;Magazine&quot;),
          TEXT({!Get_Triggering_Campaign.Promotion_Type__c})=&quot;Magazine&quot;), {!Object_Choice_Magazine}, 
IF(OR(CONTAINS({!Get_Triggering_Campaign.Name},&quot;Newsletter&quot;),
          TEXT({!Get_Triggering_Campaign.Promotion_Type__c})=&quot;Newsletter&quot;), {!Object_Choice_Newsletter}, 
IF({!Get_Triggering_Campaign.Paper__c}&lt;&gt;null, {!Object_Choice_Paper}, 
IF({!Get_Triggering_Campaign.Event__c}&lt;&gt;null, {!Object_Choice_Webinar}, 
&quot;&quot;)))))))</expression>
    </formulas>
