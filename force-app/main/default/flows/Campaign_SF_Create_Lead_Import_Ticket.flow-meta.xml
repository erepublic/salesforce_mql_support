    <formulas>
        <name>f_Date_if_Not_Weekday</name>
        <dataType>Date</dataType>
        <expression>IF(
OR(WEEKDAY({!f_Due_Date_Requested})=1,WEEKDAY({!f_Due_Date_Requested})=7),  ({!f_Due_Date_Requested}+2), {!f_Due_Date_Requested})</expression>
    </formulas>
    <formulas>
