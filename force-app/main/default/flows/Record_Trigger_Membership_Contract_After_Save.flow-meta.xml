<?xml version="1.0" encoding="UTF-8" ?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Has_the_Opportunity_Product_Changed</name>
        <label>Has the Opportunity Product Changed?</label>
        <locationX>314</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Opportunity_Product_Updated</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference
        >$Record.Opportunity_Product__r.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Product</targetReference>
            </connector>
            <label>Opportunity Product Updated</label>
        </rules>
        <rules>
            <name>Opportunity_Product_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference
        >$Record.Opportunity_Product__r.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
        >$Record.Opportunity_Product__r.Id</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference
        >Clear_Opportunity_Product_Fields</targetReference>
            </connector>
            <label>Opportunity Product Null</label>
        </rules>
    </decisions>
    <description>The one where Ryan broke stuff.</description>
    <environments>Default</environments>
    <formulas>
        <name>f_Prior_Opportunity_Product</name>
        <dataType>String</dataType>
        <expression>{!$Record__Prior.Opportunity_Product__r.Id}</expression>
    </formulas>
    <formulas>
        <name>f_Stage_to_Delivery_Status</name>
        <dataType>String</dataType>
        <expression>case( {!$Record.Stage__c} ,
&quot;Not Started&quot;, &quot;Not Started&quot;,
&quot;Completed&quot;, &quot;Completed&quot;,
&quot;Canceled&quot;, &quot;Cancelled&quot;,
&quot;In Progress&quot;)</expression>
    </formulas>
    <interviewLabel
  >Record-Trigger: Membership Contract - After Save {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Membership Contract - RT AS - Update Opportunity Product</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description
    >Update Salesforce records using values from the flow.</description>
        <name>Clear_Opportunity_Product_Fields</name>
        <label>Clear Opportunity Product &amp; Fields</label>
        <locationX>314</locationX>
        <locationY>431</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>f_Prior_Opportunity_Product</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Client_Testimonial__c</field>
            <value>
                <stringValue />
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Custom_ROI__c</field>
            <value>
                <stringValue />
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Delivery_Status_Notes__c</field>
            <value>
                <stringValue />
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Delivery_Status__c</field>
            <value>
                <stringValue />
            </value>
        </inputAssignments>
        <object>OpportunityLineItem</object>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Opportunity_Product</name>
        <label>Update Opportunity Product</label>
        <locationX>50</locationX>
        <locationY>431</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Membership_Contract__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Client_Testimonial__c</field>
            <value>
                <elementReference
        >$Record.Client_Testimonial__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Custom_ROI__c</field>
            <value>
                <elementReference>$Record.Custom_ROI__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Delivery_Status_Notes__c</field>
            <value>
                <elementReference
        >$Record.Delivery_Status_Notes__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Delivery_Status__c</field>
            <value>
                <elementReference>f_Stage_to_Delivery_Status</elementReference>
            </value>
        </inputAssignments>
        <object>OpportunityLineItem</object>
    </recordUpdates>
    <start>
        <locationX>188</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference
      >Has_the_Opportunity_Product_Changed</targetReference>
        </connector>
        <filterLogic>or</filterLogic>
        <filters>
            <field>Opportunity_Product__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Delivery_Status_Notes__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Client_Testimonial__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Custom_ROI__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Membership_Contract__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>
