    <formulas>
        <name>EstimatedCostHistoryUpdateAfterPOCreated</name>
        <dataType>String</dataType>
        <expression>TEXT(TODAY()) &amp; &quot; - &quot; &amp;
&quot; &quot; &amp; {!$User.FirstName} &amp; &quot; &quot; &amp; {!$User.LastName} &amp;
&quot; submitted PO to increase estimated cost to $&quot; &amp;
IF( {!New_Estimated_Cost}&gt;= 1000000, TEXT(FLOOR({!New_Estimated_Cost} / 1000000)) &amp; &quot;,&quot;, &quot;&quot; 
) &amp; IF({!New_Estimated_Cost} &gt;= 1000,  RIGHT(TEXT(FLOOR({!New_Estimated_Cost} / 1000)), 3) &amp; &quot;,&quot;,  &quot;&quot; ) &amp; 
RIGHT(TEXT(FLOOR({!New_Estimated_Cost})), 3) &amp; &quot;.&quot; &amp; 
IF( MOD({!New_Estimated_Cost}, 1) * 100 &lt; 10, 
  &quot;0&quot; &amp; TEXT(ROUND(MOD({!New_Estimated_Cost}, 1), 2) * 100), 
  TEXT(MIN(ROUND(MOD({!New_Estimated_Cost}, 1), 2) * 100, 99)))


 &amp;
BR() &amp;
{!Triggering_LF_Record.Estimated_Cost_History__c}</expression>
    </formulas>
    <formulas>
        <description
    >fills in the note with the reason for the change if not is blank on an increase</description>
        <name>f_Other_Note</name>
        <dataType>String</dataType>
        <expression>IF(
    CONTAINS(
        LOWER(TEXT({!Triggering_LF_Record.Category__c})),
        &quot;other&quot;
    ) &amp;&amp;
    ISBLANK({!Triggering_LF_Record.Notes__c}),
    {!Reason_for_Estimated_Cost_Change},
    {!Triggering_LF_Record.Notes__c}
)</expression>
    </formulas>
    <formulas>
        <name>NullValueFormula</name>
        <dataType>String</dataType>
        <expression>NULL</expression>
    </formulas>
    <formulas>
        <name>PurchaseOrderAmountFormula</name>
        <dataType>Currency</dataType>
        <expression
    >IF(ISNULL({!Triggering_LF_Record.Estimated_Cost__c})&amp;&amp;ISNULL({!Triggering_LF_Record.Budget__c}), {!New_Estimated_Cost},
IF(ISNULL({!Triggering_LF_Record.Estimated_Cost__c}), {!New_Estimated_Cost}-{!Triggering_LF_Record.Budget__c},
{!New_Estimated_Cost}-{!Triggering_LF_Record.Estimated_Cost__c}
))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>PurchaseOrderItemDescriptionFormula</name>
        <dataType>String</dataType>
        <expression
    >&quot;Increase in &quot;&amp;TEXT({!Triggering_LF_Record.Category__c})&amp;&quot; cost due to: &quot;&amp;{!Reason_for_Estimated_Cost_Change}</expression>
    </formulas>
    <formulas>
