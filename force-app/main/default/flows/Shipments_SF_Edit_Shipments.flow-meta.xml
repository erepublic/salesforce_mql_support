    <decisions>
        <name>Were_any_shipments_selected</name>
        <label>Were any shipments selected?</label>
        <locationX>182</locationX>
        <locationY>158</locationY>
        <defaultConnector>
            <targetReference>Error_Screen_No_Shipment</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Shipments Selected</defaultConnectorLabel>
        <rules>
            <name>Yes_shipments_selected</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ids</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_over_Selected_Shipments</targetReference>
            </connector>
            <label>Yes, shipments selected</label>
        </rules>
    </decisions>
    <dynamicChoiceSets>
