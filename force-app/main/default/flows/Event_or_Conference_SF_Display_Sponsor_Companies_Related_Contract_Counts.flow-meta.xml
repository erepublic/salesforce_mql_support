                <elementReference
        >Get_Triggering_Event_Record.Name</elementReference>
            </value>
        </filters>
        <filters>
            <field>Sponsorship_Level__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Speaker</stringValue>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Canceled</stringValue>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>Contract</object>
        <sortField>Sponsorship_Name__c</sortField>
        <sortOrder>Asc</sortOrder>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <name>Get_Current_Year_s_Other_Contracts</name>
        <label>Get Current Year&apos;s Other Contracts</label>
        <locationX>616</locationX>
        <locationY>458</locationY>
        <assignNullValuesIfNoRecordsFound
    >false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Assign_Contract_Information</targetReference>
        </connector>
        <filterLogic
    >1 AND 2 AND 3 AND 4 AND (5 OR 6) AND (7 OR 8 OR 9)</filterLogic>
        <filters>
            <field>AccountId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference
        >Loop_over_Associated_Event_Contracts.Account.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>Event_Name__c</field>
            <operator>Contains</operator>
            <value>
                <elementReference>f_Event_Year</elementReference>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Canceled</stringValue>
            </value>
        </filters>
        <filters>
            <field>Event_Type__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Standard_Teleconference - no website</stringValue>
            </value>
        </filters>
        <filters>
            <field>Event_Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Virtual Summit</stringValue>
            </value>
        </filters>
        <filters>
            <field>Event_Type__c</field>
            <operator>StartsWith</operator>
            <value>
                <stringValue>Standard</stringValue>
            </value>
        </filters>
        <filters>
            <field>Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>EE</stringValue>
            </value>
        </filters>
        <filters>
            <field>Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>GT</stringValue>
            </value>
        </filters>
        <filters>
            <field>Type__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>CDE</stringValue>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>Contract</object>
        <queriedFields>Id</queriedFields>
        <queriedFields>Event_Name__c</queriedFields>
        <queriedFields>AccountId</queriedFields>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
