    <decisions>
        <name>Check_Count_Up</name>
        <label>Check Count Up</label>
        <locationX>616</locationX>
        <locationY>890</locationY>
        <defaultConnector>
            <targetReference
      >Loop_over_Associated_Event_Contracts</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Count_up_Less_Than_90</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Count_Up</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>90.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference
        >Loop_over_Associated_Event_Contracts</targetReference>
            </connector>
            <label>Count up &lt; 90</label>
        </rules>
        <rules>
            <name>Count_up_Equals_90</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Count_Up</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>90.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Needed_a_Breather_Screen</targetReference>
            </connector>
            <label>Count up = 90</label>
        </rules>
        <rules>
            <name>Count_up_Equals_180</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Count_Up</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>180.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Needed_a_Breather_Screen_2</targetReference>
            </connector>
            <label>Count up = 180</label>
        </rules>
    </decisions>
    <description
  >Displays Event Sponsors along with a count of their Event Contracts for the current year and a detail link for more info.</description>
    <environments>Default</environments>
    <formulas>
        <name>f_Event_Contracts_Report_Link</name>
        <dataType>String</dataType>
        <expression
    >&quot;/lightning/r/Report/00O5Y00000DvbtNUAR/view?fv0=&quot; &amp; {!var_Contract_AccountId}</expression>
    </formulas>
    <formulas>
        <description
    >Formula to pull the Event Year as Text for Event Name filter.</description>
        <name>f_Event_Year</name>
        <dataType>String</dataType>
        <expression
    >TEXT({!Get_Triggering_Event_Record.Event_Year__c})</expression>
    </formulas>
    <formulas>
        <name>f_IsEvenRowStyle</name>
        <dataType>String</dataType>
        <expression
    >IF({!var_IsEvenRow_Static}=TRUE, &quot;background-color: #f2f2f2;&quot;, &quot;background-color: #ffffff;&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_Link_to_Sponsor_Contract</name>
        <dataType>String</dataType>
        <expression
    >&quot;/lightning/r/Contract/&quot; &amp; {!var_Sponsorship_Contract_Id} &amp; &quot;/view&quot;</expression>
    </formulas>
    <formulas>
        <name>f_NotIsEvenRow</name>
        <dataType>Boolean</dataType>
        <expression>IF({!var_Is_Even_Row}=False, True, False)</expression>
    </formulas>
