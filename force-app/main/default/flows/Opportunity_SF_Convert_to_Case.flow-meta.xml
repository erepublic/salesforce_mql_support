    <description>9/13/2024 (v8) - Corrected typo in Case Subject.</description>
    <environments>Default</environments>
    <formulas>
        <description
    >If Lead Source Detail is Techwire or Insider, set Insider Support Record Type, otherwise, set Navigator Support Request Record Type.</description>
        <name>f_Case_Record_Type</name>
        <dataType>String</dataType>
        <expression
    >IF(OR(CONTAINS({!Get_Opportunity.Lead_Source_Detail__c}, &quot;Techwire&quot;), CONTAINS({!Get_Opportunity.Lead_Source_Detail__c}, &quot;Insider&quot;)), &quot;0125Y000001TJSDQA4&quot;, /* If Lead Source Detail is Techwire or Insider, set Insider Support Record Type */
&quot;01230000000N5lKAAS&quot;) /* Otherwise, set Navigator Support Request Record Type */</expression>
    </formulas>
    <formulas>
        <name>f_Case_Subject</name>
        <dataType>String</dataType>
        <expression
    >{!var_Case_Record_to_Create.Subject} &amp; &quot; - Case #&quot; &amp; {!var_Case_Number}</expression>
    </formulas>
    <formulas>
        <name>f_Link_to_Case</name>
        <dataType>String</dataType>
        <expression
    >&quot;https://erepublic.lightning.force.com/lightning/r/Case/&quot; &amp; {!var_Case_Record_to_Create.Id} &amp; &quot;/view&quot;</expression>
    </formulas>
    <formulas>
        <name>f_Opportunity_Date</name>
        <dataType>String</dataType>
        <expression>TEXT(MONTH(DATEVALUE( {!Get_Opportunity.CreatedDate} ) ) ) 
&amp; &quot;/&quot; &amp; 
TEXT(DAY(DATEVALUE( {!Get_Opportunity.CreatedDate} ) ) ) 
&amp; &quot;/&quot; &amp; 
TEXT(YEAR( DATEVALUE( {!Get_Opportunity.CreatedDate} ) ) )</expression>
    </formulas>
