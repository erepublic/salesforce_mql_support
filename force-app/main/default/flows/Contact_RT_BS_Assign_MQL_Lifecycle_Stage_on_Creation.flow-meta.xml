<?xml version="1.0" encoding="UTF-8" ?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>66.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Assign_Engaged_Status</name>
        <label>Assign Engaged Status</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference
      >$Record.MQL_Lifecycle_Stage__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Engaged</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <assignments>
        <name>Assign_Unengaged_Status</name>
        <label>Assign Unengaged Status</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference
      >$Record.MQL_Lifecycle_Stage__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Unengaged</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <name>Engaged_or_Unengaged</name>
        <label>Engaged or Unengaged?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Assign_Unengaged_Status</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Unengaged</defaultConnectorLabel>
        <rules>
            <name>Engaged</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference
        >$Record.HubSpot_Private_Sector_Behavior_Score__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Engaged_Status</targetReference>
            </connector>
            <label>Engaged</label>
        </rules>
    </decisions>
    <description
  >Assign &quot;Engaged&quot; or &quot;Unengaged&quot; lifecycle stage for Private Sector contacts on creation, depending on HubSpot Private Sector Behavior Score.</description>
    <environments>Default</environments>
    <interviewLabel
  >Contact - RT BS - Assign MQL Lifecycle Stage on Creation {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Contact - RT BS - Assign MQL Lifecycle Stage on Creation</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Engaged_or_Unengaged</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>012300000008caRAAQ</stringValue>
            </value>
        </filters>
        <filters>
            <field>MQL_Lifecycle_Stage__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Contact</object>
        <recordTriggerType>Create</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
