    </formulas>
    <formulas>
        <name>f_Campaign_Start_and_End_Date</name>
        <dataType>Date</dataType>
        <expression>CASE( 
MOD({!$Record.Event_Start_Date__c} - DATE( 1900, 1, 7 ), 7 ), 
0, {!$Record.Event_Start_Date__c}- 41, 
1, {!$Record.Event_Start_Date__c}- 42, 
2, {!$Record.Event_Start_Date__c}- 42, 
3, {!$Record.Event_Start_Date__c}- 42, 
4, {!$Record.Event_Start_Date__c}- 42, 
5, {!$Record.Event_Start_Date__c}- 42, 
6, {!$Record.Event_Start_Date__c}- 43, 
null 
)</expression>
