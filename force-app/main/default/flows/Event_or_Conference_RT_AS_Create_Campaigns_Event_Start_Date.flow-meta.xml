TEXT({!$Record.Brand__c}) ,
&quot;GOVERNING&quot;, &quot;GOV&quot;,
&quot;GOVTECH&quot;, &quot;GT&quot;,
&quot;PCIO&quot;,&quot;PCIO&quot;,
&quot;CDE&quot;,&quot;CDE&quot;,
