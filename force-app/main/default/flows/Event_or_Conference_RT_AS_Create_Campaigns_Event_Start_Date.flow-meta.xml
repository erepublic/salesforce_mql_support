        <defaultConnectorLabel>No, Event has Passed</defaultConnectorLabel>
        <rules>
            <name>Yes_Event_Start_Date_is_Greater_than_Today</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference
        >$Record.Event_Start_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Flow.CurrentDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference
        >Create_Call_for_Sponsors_2_Campaign</targetReference>
            </connector>
            <label>Yes, Event Start Date is Greater than Today</label>
        </rules>
    </decisions>
