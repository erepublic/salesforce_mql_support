    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>What_Account_Name_RecordType</targetReference>
        </connector>
        <filterFormula>AND(
         {!$User.UserRoleId} &lt;&gt; &quot;00E5Y000002pWGI&quot;, /* Exclude BizOps Flow User Role */
         {!$Record.Owner:User.UserRoleId} &lt;&gt; &quot;00E5Y000002pWGI&quot;, /* Exclude BizOps Owner Role as Owners */
         OR(
                {!$Record.OwnerId} &lt;&gt; {!$Record.Contact__r.Account.OwnerId},
                CONTAINS({!$Record.Contact__r.Account.Name}, &quot;e.Republic&quot;)
               )
        )</filterFormula>
