        </rules>
    </decisions>
    <description
  >Launches from the &apos;Account Admin: Manage&apos; link. Assigns account to a specific Owner/Status based on selection.</description>
    <dynamicChoiceSets>
        <name>Users</name>
        <dataType>String</dataType>
        <displayField>Name</displayField>
        <filterLogic>and</filterLogic>
        <filters>
            <field>IsActive</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>ProfileId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>00e30000001cUuX</stringValue>
            </value>
        </filters>
        <object>User</object>
        <outputAssignments>
            <assignToReference>UserId</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>UserName</assignToReference>
            <field>Name</field>
        </outputAssignments>
        <sortField>Name</sortField>
        <sortOrder>Asc</sortOrder>
        <valueField>Id</valueField>
    </dynamicChoiceSets>
    <environments>Default</environments>
    <formulas>
        <description
    >WARNING - if you are currently in Sandbox, this will link to production.</description>
        <name>f_Link_to_Account</name>
        <dataType>String</dataType>
        <expression
    >&quot;https://erepublic.lightning.force.com/lightning/r/Account/&quot; &amp; {!AccountId} &amp; &quot;/view&quot;</expression>
    </formulas>
    <formulas>
        <description
    >Assign to Tiffany Cianci for &quot;Not a Good Fit&quot; and &quot;Out of Business&quot;. Assign to Erin Molina for &quot;Nurture&quot;</description>
        <name>f_Which_Account_Owner</name>
        <dataType>String</dataType>
        <expression>CASE({!Screen_Change_to}, 
          &quot;Not a Good Fit&quot;, &quot;005a0000009u4TkAAI&quot;,
          &quot;Nurture&quot;, &quot;005a0000009aZsm&quot;,
          &quot;Out of Business&quot;, &quot;005a0000009u4TkAAI&quot;, 
          &quot;005a0000009u4TkAAI&quot;
)</expression>
    </formulas>
