    <decisions>
        <name>Which_Selection</name>
        <label>Which Selection?</label>
        <locationX>856</locationX>
        <locationY>349</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Out_of_Business_Selected</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Screen_Change_to</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Out_of_Business</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference
        >Update_Account_Out_of_Business</targetReference>
            </connector>
            <label>Out of Business Selected</label>
        </rules>
        <rules>
            <name>Not_a_Good_Fit_Selected</name>
