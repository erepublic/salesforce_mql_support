<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_and_Name_with_Link__c</fullName>
    <description
  >shows the date and name of the event and links to the event record for use on reports that won&apos;t be exported only</description>
    <externalId>false</externalId>
    <formula
  >LPAD(TEXT(MONTH(Event_Start_Date__c)), 2,&quot;0&quot;)&amp;&quot;-&quot;&amp; 
LPAD(TEXT(DAY(Event_Start_Date__c)), 2,&quot;0&quot;)&amp;&quot; - &quot;&amp; 
HYPERLINK(&quot;/&quot;&amp;Id,Name)</formula>
    <label>Date and Name (with Link)</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
