<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Executive_Briefing_Start_Time</fullName>
    <active>true</active>
    <description
  >Executive Briefing Start Time must have a valid time format.</description>
    <errorConditionFormula>NOT(
ISBLANK(Executive_Briefing_Start_Time__c)
)
&amp;&amp;
NOT(
REGEX(
Executive_Briefing_Start_Time__c ,
&quot;([1-9]|1[0-2]):[0-5][0-9] [A|P]M&quot;
)
)</errorConditionFormula>
    <errorDisplayField>Executive_Briefing_Start_Time__c</errorDisplayField>
    <errorMessage
  >Invalid time format. Use 10:30 AM / 1:30 PM format.</errorMessage>
</ValidationRule>
