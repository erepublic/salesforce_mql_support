<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Course_Account_Mismatch__c</fullName>
    <description
  >Marked true if the Course Enrollment Name does not contain the Course Account Name.</description>
    <formula>NOT(CONTAINS(Name,  Course_Account_Name__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Course Account Mismatch</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
