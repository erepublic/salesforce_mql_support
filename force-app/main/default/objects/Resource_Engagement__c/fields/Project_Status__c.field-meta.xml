<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_Status__c</fullName>
    <description
  >Created for Ticket #81677 to designate if the Project on a Resource Engagement is Open when reviewing an &quot;Erase My Data&quot; Privacy Removal Request.</description>
    <externalId>false</externalId>
    <formula>IF(AND(
         TEXT(Project__r.Project_Phase__c)&lt;&gt;&quot;Post-Production Required&quot;,
         TEXT(Project__r.Project_Phase__c)&lt;&gt;&quot;Completed&quot;,
         TEXT(Project__r.Project_Phase__c)&lt;&gt;&quot;Cancelled&quot;,
         TEXT(Project__r.Project_Stage__c)&lt;&gt;&quot;Completed&quot;,
         TEXT(Project__r.Project_Stage__c)&lt;&gt;&quot;Cancelled&quot;,
         TEXT(Project__r.Project_Stage__c)&lt;&gt;&quot;Closed&quot;

         ), &quot;Open&quot;, &quot;Closed&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Project Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
