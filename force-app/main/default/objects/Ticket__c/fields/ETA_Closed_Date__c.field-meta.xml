<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ETA_Closed_Date__c</fullName>
    <description
  >Shows the date closed if there is one, or the ETA if there is not.</description>
    <formula>IF(NOT(ISBLANK(Date_Closed__c)), Date_Closed__c, ETA__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Date Closed or ETA</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
