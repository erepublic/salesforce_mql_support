<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phase_Age__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Project_Phase__c, &quot;Completed&quot;)||
ISPICKVAL(Project_Phase__c, &quot;Cancelled&quot;),(
Completion_Date__c - (Phase_Date_Last_Updated__c)),
TODAY() - (Phase_Date_Last_Updated__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Phase Age</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
